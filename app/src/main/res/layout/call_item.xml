<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_horizontal_margin">

    <TextView
        android:id="@+id/client_call_log_text_view"
        android:layout_width="191dp"
        android:layout_height="19dp"
        android:text="James Bond"
        android:layout_marginLeft="8dp"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/call_type_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="incoming"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/client_call_log_text_view" />

    <TextView
        android:id="@+id/call_date_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="20.10.17"
        app:layout_constraintTop_toTopOf="@+id/client_call_log_text_view"
        android:layout_marginTop="0dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/call_duration_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="7 seconds"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/call_date_text_view"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/call_description_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="description"
        android:textColor="@android:color/black"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="76dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/call_result_text_view" />

    <TextView
        android:id="@+id/call_result_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/call_type_text_view"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>